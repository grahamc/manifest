<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote
    name="origin"
    fetch="ssh://git@github.com/"
    />
  <default
    remote="origin"
    revision="master"
    sync-j="8"
    />

  <project name="grahamc/manifest" path="projects/manifest">
    <linkfile src="bin" dest="bin" />

    <linkfile src="profile.sh" dest=".profile" />
    <linkfile src="zshrc" dest=".zshrc" />

    <linkfile src="emacs/init.el" dest=".emacs.d/init.el" />

    <linkfile src="git/config" dest=".gitconfig" />
    <linkfile src="git/gitignore-global" dest=".gitignore-global" />
  </project>

  <project name="robbyrussell/oh-my-zsh" path=".oh-my-zsh" />

  <project name="grahamc/shoutcast" path="projects/shoutcast" />
</manifest>
