<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote
    name="origin"
    fetch=".."
    />
  <default
    remote="origin"
    revision="master"
    sync-j="8"
    />

  <project name="manifest" path="projects/manifest">
    <linkfile src="bin" dest="bin" />
    <linkfile src="profile.sh" dest=".profile" />

    <linkfile src="git/config" dest=".gitconfig" />
    <linkfile src="git/gitignore-global" dest=".gitignore-global" />
  </project>
</manifest>
  
